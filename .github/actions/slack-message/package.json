{
  "name": "wrapper-slack-message",
  "version": "0.0.0",
  "private": true,
  "description": "Wrapper Slack Message",
  "main": "lib/main.js",
  "scripts": {
    "check": "checkIsUnchanged ./dist/index.js",
    "lint": "eslint src/**/*.ts",
    "fmt": "prettier --write '**/*.ts'",
    "build": "tsc",
    "test": "jest",
    "package": "ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "all": "npm run build && npm run fmt && npm run lint && npm run package && npm test"
  },
  "keywords": [
    "github",
    "actions",
    "node",
    "typescript"
  ],
  "author": "elastic/oblt-robots",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "^1.10.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@types/node": "^18.15.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.38.0",
    "eslint-plugin-github": "^4.7.0",
    "eslint-plugin-jest": "^27.2.1",
    "github-action-ts-run-api": "^3.0.3",
    "github-actions-utils": "^1.0.9",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
